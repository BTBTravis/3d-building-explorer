{
  "name": "3D_Building_Explorer",
  "author": "Travis Shears",
  "homepage": "https://github.com/BTBTravis/3d-building-explorer",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A library for turning SketchUp models into explore-able web experiences via Three.js",
  "scripts": {
    "build": "env NODE_ENV=rollingup rollup -c",
    "dev": "env NODE_ENV=rollingup rollup -c -w",
    "test": "env NODE_ENV=testing mocha --compilers js:babel-register --require babel-polyfill ./test/test.js",
    "test-watch": "env NODE_ENV=testing mocha -w --compilers js:babel-register --require babel-polyfill ./test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BTBTravis/3d-building-explorer.git"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "mocha": "^5.0.5",
    "rollup": "^0.57.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "keywords": [
    "three.js",
    "3d",
    "building",
    "explorer",
    "html",
    "javascript",
    "sketchup",
    "blender"
  ],
  "dependencies": {
    "gsap": "^1.20.3",
    "three": "^0.89.0",
    "three-collada-loader": "0.0.1",
    "three-orbit-controls": "^82.1.0"
  },
  "files": [
    "dist"
  ]
}
